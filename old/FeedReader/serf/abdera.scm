;; defines the AbderaParser class that constructs the list of entries of an atom feed
(define-java-class <java-abdera-parser> |AbderaParser|)
(define-generic-java-method get-entry-list)

;; An Atom feed comprises a sequence of zero or more Entry objects.
(define-java-class <java-abdera-model-entry> |org.apache.abdera.model.Entry|)
(define-generic-java-method get-author)  ; Person getAuthor() for Entry
(define-generic-java-method get-authors) ; List<Person> getAuthors() for Entry
(define-generic-java-method get-content) ; String getContent() for Entry
(define-generic-java-method get-content-element) ; Content getContentElement() for Entry
(define-generic-java-method get-content-type) ; Content.Type getContentType() for Entry
(define-generic-java-method get-id)           ; IRI getId() for Entry
(define-generic-java-method get-links) ; List<Link> getLinks() for Entry

(define-generic-java-method get-published) ; Date getPublished() for Entry
(define-generic-java-method get-title) ; String getTitle() for Entry
(define-generic-java-method get-updated) ; Date getUpdated() for Entry

;; Methods for Content objects.
(define-generic-java-method get-text) ; String getText() for Content

;; For example, the get-authors method returns a list of Person objects.
;(define-java-class <java-abdera-model-person> |org.apache.abdera.model.Person|)
(define-generic-java-method get-email) ; String getEmail() for Person
(define-generic-java-method get-name)  ; String getName() for Person

;; Date methods.
(define-generic-java-method get-time) ; long getTime() for Date [returns milliseconds since start of epoch].

;; Defines the Link class that is returned when calling the get-link method of the Entry object.
;(define-java-class <java-abdera-model-link> |org.apache.abdera.model.Link|)
(define-generic-java-method get-href)
(define-generic-java-method get-mime-type)

;; Methods for java.util.List
(define-generic-java-method get)  ; Object get(int);
(define-generic-java-method size) ; int size()
(define-generic-java-method list-iterator) ; ListIterator listIterator()
(define-generic-java-method has-next) ; boolean hasNext() for ListIterator
(define-generic-java-method next) ; Object next() for ListIterator

;; Convert a Java List<T> [t_0, t_1, ..., t_{n-1}] to a Scheme list (t_0 t_1 ... t_{n-1})
(define (reify-java-list x)
  (let loop ((i (- (->number (size x)) 1))
	     (accumulate '()))
    (cond
     ((>= i 0)
      (loop (- i 1) (cons (get x (->jint i)) accumulate)))
     (else accumulate))))

;; Constructs the parsed Atom feed given by string s.
(define (abdera/parse s)
  (java-new <java-abdera-parser> (->jstring s)))

;; Returns the individual Entry objects in the feed as a list in the order
;; in which they appear in the feed.
(define (abdera/feed/list feed)
  (reify-java-list (get-entry-list feed)))

(define (abdera/entry/author entry)
  (get-author entry))

(define (abdera/entry/title entry)
  (->string (get-title entry)))

(define (abdera/entry/content entry)
  (->string (get-content entry)))

(define (abdera/entry/links entry)
  (reify-java-list (get-links entry)))

(define (java-date-to-utc d)
  (let* ((milliseconds (->number (get-time d)))
	 (seconds (quotient milliseconds 1000)))
    (make-time time-utc 0 seconds)))

(define (abdera/entry/published entry)
  (java-date-to-utc (get-published entry)))

(define (abdera/entry/updated entry)
  (java-date-to-utc (get-updated entry)))

(define (abdera/link/href link)
  (->string (get-href link)))

(define (abdera/link/title link)
  (let ((title (get-title link)))
    (if (java-null? title) #f (->string title))))

(define (abdera/person/name person)
  (->string (get-name person)))

(define (abdera/person/email person)
  (->string (get-email person)))

(define :gpo-atom-feed: "<?xml version='1.0' encoding='UTF-8'?><?xml-stylesheet href=\"http://www.blogger.com/styles/atom.css\" type=\"text/css\"?><feed xmlns='http://www.w3.org/2005/Atom' xmlns:openSearch='http://a9.com/-/spec/opensearchrss/1.0/' xmlns:georss='http://www.georss.org/georss'><id>tag:blogger.com,1999:blog-10013556</id><updated>2009-07-11T19:01:06.792-04:00</updated><title type='text'>FDsys</title><subtitle type='html'>GPO's Federal Digital System (FDsys)is an advanced digital system enabling GPO to manage Government information in a digital form from all three branches of Government.</subtitle><link rel='http://schemas.google.com/g/2005#feed' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/posts/default'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/'/><link rel='next' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default?start-index=26&amp;max-results=25'/><author><name>Scott Stovall</name><email>noreply@blogger.com</email></author><generator version='7.00' uri='http://www.blogger.com'>Blogger</generator><openSearch:totalResults>88</openSearch:totalResults><openSearch:startIndex>1</openSearch:startIndex><openSearch:itemsPerPage>25</openSearch:itemsPerPage><entry><id>tag:blogger.com,1999:blog-10013556.post-8194732216431650644</id><published>2009-06-11T08:18:00.000-04:00</published><updated>2009-06-11T08:19:09.907-04:00</updated><title type='text'>New Collections Added</title><content type='html'>GPO is pleased to announce that four new collections are now available from the Federal Digital System (FDsys) at &lt;a href=\"http://www.fdsys.gov/\"&gt;www.fdsys.gov&lt;/a&gt;.&lt;br /&gt;&lt;br /&gt;These new collections are:&lt;br /&gt;&lt;ul&gt;&lt;li&gt;Congressional Calendars (104th Congress - Present)&lt;/li&gt;&lt;li&gt;Congressional Committee Prints (105th Congress - Present)&lt;/li&gt;&lt;li&gt;List of CFR Sections Affected (1997 - Present)&lt;/li&gt;&lt;li&gt;Economic Indicators (1995 - Present)&lt;/li&gt;&lt;/ul&gt;&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-8194732216431650644?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/8194732216431650644/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=8194732216431650644' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/8194732216431650644'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/8194732216431650644'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/06/new-collections-added.html' title='New Collections Added'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-1398078742732283077</id><published>2009-06-01T09:14:00.001-04:00</published><updated>2009-06-01T09:19:21.044-04:00</updated><title type='text'>Updates to Retrieve by Citation Feature</title><content type='html'>Responding to feedback received since launch, the result from the Retrieve by Citation search now opens to the requested page, instead of the beginning page of the document.&lt;br /&gt;For example, a search for page 19097 in Volume 74 of the Federal Register opens &lt;a href=\"http://fdsys.gpo.gov/fdsys/pkg/FR-2009-04-27/pdf/E9-9474.pdf#page=1\"&gt;the result&lt;/a&gt; in a new window.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-1398078742732283077?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/1398078742732283077/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=1398078742732283077' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1398078742732283077'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1398078742732283077'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/06/updates-to-retrieve-by-citation-feature.html' title='Updates to Retrieve by Citation Feature'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-1794210722671931801</id><published>2009-05-15T13:38:00.001-04:00</published><updated>2009-05-15T13:40:04.152-04:00</updated><title type='text'>Tip:  Bookmarking Frequent Searches</title><content type='html'>FDsys has the capability to bookmark search results, including persisted sorts and filters. This feature may be used in many ways, but I will highlight a search that I have bookmarked.&lt;br /&gt;&lt;a href=\"http://fdsys.gpo.gov/fdsys/search/search.action?na=_accodenav_billversionnav_congressnumnav_&amp;amp;se=_BILLSfalse_ENRtrue_111true_&amp;amp;sm=&amp;amp;flr=2009false&amp;amp;ercode=&amp;amp;dateBrowse=&amp;amp;st=H.R.&amp;amp;=H.R.&amp;amp;psh=&amp;amp;sbh=&amp;amp;tfh=&amp;amp;originalSearch=H.R.&amp;amp;sb=dno&amp;amp;ps=100&amp;amp;sb=dno&amp;amp;ps=100\"&gt;Enrolled Bills of the 111th Congress&lt;/a&gt;&lt;br /&gt;The search result refreshes when the page opens, allowing me one-click access to all Congressional Bills signed into law during the current Congress. Notice that my sort preferences are also saved (sort by date and results per page).&lt;br /&gt;FDsys will offer alert capabilities such as RSS and email notifications in Release 3. We hope that the bookmarking feature will assist you until the alert capabilities are implemented.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-1794210722671931801?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/1794210722671931801/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=1794210722671931801' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1794210722671931801'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1794210722671931801'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/05/tip-bookmarking-frequent-searches.html' title='Tip:  Bookmarking Frequent Searches'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-6170426467695660245</id><published>2009-05-07T08:11:00.000-04:00</published><updated>2009-05-07T08:12:03.665-04:00</updated><title type='text'>FDsys Update at Spring DLC Conference</title><content type='html'>On Tuesday, April 21, GPO presented an update on FDsys development at the Spring Depository Library Council meeting. The presentation included an overview of the implementation approach and beta feedback, as well as a review of enhancements implemented since launch. The presentation is available &lt;a href=\"http://fdsys.gpo.gov/ppt/FDsys%20Spring%20DLC%202009.ppt\"&gt;here&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-6170426467695660245?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/6170426467695660245/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=6170426467695660245' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6170426467695660245'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6170426467695660245'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/05/fdsys-update-at-spring-dlc-conference.html' title='FDsys Update at Spring DLC Conference'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-2606900271860809339</id><published>2009-04-29T08:52:00.002-04:00</published><updated>2009-04-29T09:08:48.598-04:00</updated><title type='text'>Quick Link to Today’s Issue of the Federal Register</title><content type='html'>We have had a number requests for a quick link to the current day’s issue of the &lt;em&gt;Federal Register&lt;/em&gt; and are happy to report that the feature is now active. A link for Today’s Issue of the Federal Register is available at the top of the &lt;a href=\"http://fdsys.gpo.gov/fdsys/browse/collection.action?collectionCode=FR\"&gt;Federal Register Browse page&lt;/a&gt;. You can also bookmark the link, &lt;a href=\"http://fdsys.gpo.gov/fdsys/search/getfrtoc.action\"&gt;Today’s Issue of the Federal Register&lt;/a&gt;, for one-click access to the current issue.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-2606900271860809339?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/2606900271860809339/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=2606900271860809339' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/2606900271860809339'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/2606900271860809339'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/04/quick-link-to-todays-issue-of-federal.html' title='Quick Link to Today’s Issue of the Federal Register'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-7093189489224818674</id><published>2009-04-15T13:14:00.001-04:00</published><updated>2009-04-15T13:15:33.500-04:00</updated><title type='text'>Open Letter to President Obama</title><content type='html'>In a recent letter to President Barack Obama, GPO’s Public Printer, Bob Tapella, detailed how GPO can help implement the White House’s transparency and open government agenda. Mr. Tapella included five goals and planned actions for the President to review, four of which relate to FDsys. The goals and actions include:&lt;br /&gt;&lt;br /&gt;&lt;strong&gt;Goal:&lt;/strong&gt; Position GPO’s Federal Digital System (FDsys) as the official repository for Federal Government publications.&lt;br /&gt;&lt;strong&gt;Action:&lt;/strong&gt; Develop pilot concepts with the Administration’s Open Government Office.&lt;br /&gt;&lt;br /&gt;&lt;strong&gt;Goal:&lt;/strong&gt; Enable and support Web 2.0 functionality through FDsys to support public comments on pending legislation.&lt;br /&gt;&lt;strong&gt;Action:&lt;/strong&gt; Develop a pilot with the Open Government office to evaluate the process to collect comments on selected bills.&lt;br /&gt;&lt;br /&gt;&lt;strong&gt;Goal:&lt;/strong&gt; Establish a demonstration project to apply Web 2.0 features to rulemaking documents.&lt;br /&gt;&lt;strong&gt;Action:&lt;/strong&gt; Applying lessons learned, extend the demonstration project to the entire Federal Register on FDsys in a systematic way.&lt;br /&gt;&lt;br /&gt;&lt;strong&gt;Goal:&lt;/strong&gt; Participate in and lead efforts to standardize electronic publishing formats.&lt;br /&gt;&lt;strong&gt;Action:&lt;/strong&gt; Meet with the Office of Management and Budget (OMB) e-Government and Information Group to develop guidelines for extending StratML to other Federal publication types.&lt;br /&gt;&lt;br /&gt;&lt;strong&gt;Goal:&lt;/strong&gt; Link the White House’s Web site to FDsys for public searches of Government documents.&lt;br /&gt;&lt;strong&gt;Action:&lt;/strong&gt; Contact and establish a working relationship between GPO’s FDsys and the White House Web team. This would include creating a White House/GPO working group/Web team, providing a demonstration of FDsys for the White House Web team, and establishing a recurring meeting with the White House Web team for information exchange.&lt;br /&gt;&lt;br /&gt;The complete letter is available at &lt;a title=\"http://www.gpo.gov/news/2009/03-09-09_OpenGovLtr.pdf\" href=\"http://www.gpo.gov/news/2009/03-09-09_OpenGovLtr.pdf\"&gt;http://www.gpo.gov/news/2009/03-09-09_OpenGovLtr.pdf&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-7093189489224818674?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/7093189489224818674/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=7093189489224818674' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/7093189489224818674'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/7093189489224818674'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/04/open-letter-to-president-obama.html' title='Open Letter to President Obama'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-3287908106564515258</id><published>2009-03-24T09:13:00.002-04:00</published><updated>2009-03-24T09:15:01.598-04:00</updated><title type='text'>FDsys will be down for maintenance</title><content type='html'>FDsys will be down for maintenance beginning at 10 p.m. on Friday, March 27 until noon on Monday, March 30. &lt;a href=\"http://www.gpoaccess.gov/\"&gt;GPO Access&lt;/a&gt; will remain operational during this time. We apologize for any inconvenience this may cause.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-3287908106564515258?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/3287908106564515258/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=3287908106564515258' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/3287908106564515258'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/3287908106564515258'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/03/blog-post.html' title='FDsys will be down for maintenance'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-5492146561330432616</id><published>2009-02-04T12:32:00.000-05:00</published><updated>2009-02-04T12:33:43.092-05:00</updated><title type='text'>FDsys in the Washington Post</title><content type='html'>FDsys was featured in the Federal Eye section of the Washington Post today. You can access the article &lt;a href=\"http://voices.washingtonpost.com/federal-eye/2009/02/new_government_search_site_lau.html?hpid=news-col-blog\"&gt;here&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-5492146561330432616?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/5492146561330432616/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=5492146561330432616' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/5492146561330432616'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/5492146561330432616'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/02/fdsys-in-washington-post.html' title='FDsys in the Washington Post'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-1220408342417934591</id><published>2009-02-02T14:30:00.000-05:00</published><updated>2009-02-02T14:31:32.387-05:00</updated><title type='text'>New Techline</title><content type='html'>The latest version of Techline, the FDsys newsletter, is now available on the GPO website or by clicking &lt;a href=\"http://www.gpo.gov/projects/pdfs/techline_jan2009.pdf\"&gt;here&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-1220408342417934591?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/1220408342417934591/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=1220408342417934591' title='1 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1220408342417934591'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1220408342417934591'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/02/new-techline.html' title='New Techline'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>1</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-326820692971117643</id><published>2009-01-15T14:05:00.001-05:00</published><updated>2009-01-15T15:01:08.576-05:00</updated><title type='text'>GPO Launches FDsys Public Beta</title><content type='html'>GPO is pleased to announce that FDsys is now available as a public beta. The system may be accessed through &lt;a href=\"http://fdsys.gpo.gov/\"&gt;http://fdsys.gpo.gov&lt;/a&gt;. The information on GPO Access is in the process of being migrated to GPO’s Federal Digital System (FDsys), a process that will be complete in mid-2009. The migration is occurring on a collection-by-collection basis. The information on GPO Access will remain current and continue to be available until migration is complete. Users will continue to be able to access all GPO Access content as described in the A-Z Resource List.&lt;br /&gt;&lt;br /&gt;Collections currently available for search in FDsys are:&lt;br /&gt;&lt;br /&gt;* Congressional Bills&lt;br /&gt;&lt;br /&gt;* Congressional Documents&lt;br /&gt;&lt;br /&gt;* Congressional Hearings&lt;br /&gt;&lt;br /&gt;* Congressional Record&lt;br /&gt;&lt;br /&gt;* Congressional Reports&lt;br /&gt;&lt;br /&gt;* Federal Register&lt;br /&gt;&lt;br /&gt;* Public and Private Laws&lt;br /&gt;&lt;br /&gt;* Compilation of Presidential Documents&lt;br /&gt;&lt;br /&gt;GPO is requesting your feedback on the system. All questions and comments should be directed to &lt;a title=\"mailto:pmo@gpo.gov\" href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;. For more information on the capabilities of FDsys by release, visit &lt;a title=\"http://www.gpo.gov/projects/fdsys_capabilities.htm\" href=\"http://www.gpo.gov/projects/fdsys_capabilities.htm\"&gt;http://www.gpo.gov/projects/fdsys_capabilities.htm&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-326820692971117643?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/326820692971117643/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=326820692971117643' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/326820692971117643'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/326820692971117643'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/01/gpo-launches-fdsys-public-beta.html' title='GPO Launches FDsys Public Beta'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-7236170262562684485</id><published>2009-01-05T16:14:00.000-05:00</published><updated>2009-01-05T16:15:26.760-05:00</updated><title type='text'>GPO begins beta testing</title><content type='html'>Beta testing for the first public release of FDsys began on Friday, January 2 and will continue through January 12. If you are interested in beta testing the system email &lt;a title=\"mailto:pmo@gpo.gov\" href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-7236170262562684485?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/7236170262562684485/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=7236170262562684485' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/7236170262562684485'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/7236170262562684485'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2009/01/gpo-begins-beta-testing.html' title='GPO begins beta testing'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-1083656462865593180</id><published>2008-11-17T09:17:00.000-05:00</published><updated>2008-11-17T09:20:26.614-05:00</updated><title type='text'>Search Demo - Part 4 - Boolean Search</title><content type='html'>GPO has posted Part 4 of the FDsys search demonstration which highlights Boolean searches. The video is available &lt;a href=\"http://www.youtube.com/watch?v=rloFsBt7kuc\"&gt;here&lt;/a&gt;. Comments and suggestions are always welcome on the blog or via email (&lt;a href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;). Part 5 of the series will demonstrate how to browse for documents within FDsys.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-1083656462865593180?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/1083656462865593180/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=1083656462865593180' title='4 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1083656462865593180'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1083656462865593180'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/11/search-demo-part-4-boolean-search.html' title='Search Demo - Part 4 - Boolean Search'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>4</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-3770512637133725267</id><published>2008-11-12T13:41:00.000-05:00</published><updated>2008-11-12T13:43:36.438-05:00</updated><title type='text'>Search Demo - Part 3 - Citation Search</title><content type='html'>GPO has posted Part 3 of the FDsys Search Demonstration which showcases conducting a citation search. The video is available &lt;a href=\"http://www.youtube.com/watch?v=hoYms0lQL5I\"&gt;here&lt;/a&gt;. Comments and suggestions are always welcome here or via email to &lt;a href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;. Part 4 of the search demonstration will highlight boolean searches.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-3770512637133725267?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/3770512637133725267/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=3770512637133725267' title='1 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/3770512637133725267'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/3770512637133725267'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/11/search-demo-part-3-citation-search.html' title='Search Demo - Part 3 - Citation Search'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>1</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-8901475574621798037</id><published>2008-11-06T09:11:00.000-05:00</published><updated>2008-11-06T09:16:13.112-05:00</updated><title type='text'>Advanced Search Demonstration</title><content type='html'>On Monday, we posted a short demonstration of performing a simple search within FDsys. Users will also be able to perform advanced searches in the system. To view a demonstration of an advanced search, &lt;a href=\"http://www.youtube.com/watch?v=Q5cWk_4r6RI\"&gt;click here&lt;/a&gt;. Send your comments to &lt;a href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt; or post them on the blog. We will upload Part 3 - Citation Search next week.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-8901475574621798037?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/8901475574621798037/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=8901475574621798037' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/8901475574621798037'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/8901475574621798037'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/11/advanced-search-demonstration_06.html' title='Advanced Search Demonstration'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-3889803495822491914</id><published>2008-11-03T12:24:00.000-05:00</published><updated>2008-11-03T12:25:19.317-05:00</updated><title type='text'>Simple Search Demonstration</title><content type='html'>A short demonstration on the simple search functionality of FDsys has been created and posted. &lt;a href=\"http://www.youtube.com/watch?v=I_unJs7gQJA\"&gt;View the video&lt;/a&gt; on YouTube and send your comments to &lt;a href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt; or post them on the blog. We will be uploading Part 2 - Advanced Search soon.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-3889803495822491914?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/3889803495822491914/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=3889803495822491914' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/3889803495822491914'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/3889803495822491914'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/11/simple-search-demonstration.html' title='Simple Search Demonstration'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-1062161277582025874</id><published>2008-10-27T08:50:00.001-04:00</published><updated>2008-10-27T08:53:34.530-04:00</updated><title type='text'>GPO's Depository Library Council Meeting</title><content type='html'>GPO presented FDsys updates at the 2008 Fall Depository Library Council meeting in Arlington, VA on October 21. There were two separate sessions – a technical overview of the system architecture and design, as well as an update on development, including a live demonstration of FDsys. Both presentations are available in powerpoint format through GPO’s website (&lt;a title=\"http://www.gpo.gov/projects/fdsys_presentations.htm\" href=\"http://www.gpo.gov/projects/fdsys_presentations.htm\"&gt;http://www.gpo.gov/projects/fdsys_presentations.htm&lt;/a&gt;).&lt;br /&gt;&lt;br /&gt;We also released a new version of the FDsys Newsletter, Techline, in coordination with the Council Meeting. Techline is available(&lt;a href=\"http://www.gpo.gov/projects/pdfs/techline_oct2008.pdf\"&gt;http://www.gpo.gov/projects/pdfs/techline_oct2008.pdf&lt;/a&gt;). Short videos of FDsys, similar to the demonstration given at the meeting, will be available through the blog later this week.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-1062161277582025874?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/1062161277582025874/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=1062161277582025874' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1062161277582025874'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1062161277582025874'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/10/gpos-depository-library-council-meeting.html' title='GPO&apos;s Depository Library Council Meeting'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-279656265340072276</id><published>2008-07-17T09:37:00.002-04:00</published><updated>2008-07-17T09:39:39.668-04:00</updated><title type='text'>GPO releases white paper \"Digital Preservation at the U.S. Government Printing Office\"</title><content type='html'>A new GPO paper “Digital Preservation at the U.S. Government Printing Office” is available at the FDsys Current Documents page at &lt;a href=\"http://www.gpo.gov/projects/pdfs/preservation-white-paper_20080709.pdf\"&gt;http://www.gpo.gov/projects/pdfs/preservation-white-paper_20080709.pdf&lt;/a&gt;&lt;br /&gt;&lt;br /&gt;The paper provides a non-technical introduction to digital preservation, particularly to the challenges for keeping digital content available and accessible into the future. Then the focus shifts to the digital preservation activities of the U.S. Government Printing Office (GPO) and highlights of GPO’s technological transformation, moving from Web access, to content authentication, and development of a trusted repository. The final section introduces GPO’s Federal Digital System, a content management system and digital repository designed to support GPO’s mission of keeping America informed.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-279656265340072276?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/279656265340072276/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=279656265340072276' title='3 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/279656265340072276'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/279656265340072276'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/07/gpo-releases-white-paper-digital.html' title='GPO releases white paper &quot;Digital Preservation at the U.S. Government Printing Office&quot;'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>3</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-6545989654851379293</id><published>2008-05-30T18:42:00.001-04:00</published><updated>2008-05-30T18:42:44.026-04:00</updated><title type='text'>Attention GPO Access Users</title><content type='html'>We are currently working with a usability specialist to design the user interfaces for the first public release of FDsys and we would love to hear from current &lt;em&gt;GPO Access&lt;/em&gt; users. If you use &lt;em&gt;GPO Access&lt;/em&gt; and are willing to speak to our usability contractor send an email to &lt;a href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-6545989654851379293?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/6545989654851379293/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=6545989654851379293' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6545989654851379293'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6545989654851379293'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/05/attention-gpo-access-users.html' title='Attention GPO Access Users'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-8124188824710633352</id><published>2008-04-08T09:34:00.000-04:00</published><updated>2008-04-08T09:35:26.199-04:00</updated><title type='text'>GPO Announces Program Changes</title><content type='html'>In February 2008, the U.S. Government Printing Office (GPO) assumed a broader role in the creation of GPO’s Federal Digital System (FDsys) through modifications of its contract with Harris Corporation. This change will allow GPO to manage all program responsibilities, the role of the master integrator. GPO now has more control and oversight of the program resources, schedule, and cost, while Harris Corporation maintains its significant role in the development of FDsys. In order to supplement resources GPO is bringing on additional subject matter experts from FAST, Documentum, and MITRE Corporation. The expected capabilities of FDsys have not changed and GPO’s focus remains on the first public release, targeted for late 2008.&lt;br /&gt;&lt;br /&gt;For more information on the FDsys release schedule and the capabilities of each release, visit &lt;a href=\"http://www.gpo.gov/fdsys\"&gt;www.gpo.gov/fdsys&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-8124188824710633352?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/8124188824710633352/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=8124188824710633352' title='3 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/8124188824710633352'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/8124188824710633352'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/04/gpo-announces-program-changes.html' title='GPO Announces Program Changes'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>3</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-6907080713833874143</id><published>2008-02-27T16:40:00.001-05:00</published><updated>2008-02-27T16:44:44.707-05:00</updated><title type='text'>View the FDsys Proof-of-Concept Demo</title><content type='html'>GPO has posted the FDsys proof-of-concept demonstrations to the website. The demos are separated into two files, Search and Submission.&lt;br /&gt;&lt;a href=\"http://www.gpo.gov/projects/fdsys_status.htm\"&gt;http://www.gpo.gov/projects/fdsys_status.htm&lt;/a&gt;&lt;br /&gt;The demos must be viewed with Windows Media Player. We would love to receive feedback -- you can leave comments here or send us an email at &lt;a href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-6907080713833874143?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/6907080713833874143/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=6907080713833874143' title='2 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6907080713833874143'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6907080713833874143'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/02/view-fdsys-proof-of-concept-demo.html' title='View the FDsys Proof-of-Concept Demo'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>2</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-640943319774285636</id><published>2008-02-25T11:28:00.000-05:00</published><updated>2008-02-25T11:30:17.652-05:00</updated><title type='text'>Updated Requirements Document</title><content type='html'>The FDsys Requirements Document has been updated to Version 3.2 and is now available on the FDsys website at&lt;br /&gt;&lt;a href=\"http://www.gpo.gov/projects/pdfs/FDsys_RD_v3.2.pdf\"&gt;http://www.gpo.gov/projects/pdfs/FDsys_RD_v3.2.pdf&lt;/a&gt;&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-640943319774285636?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/640943319774285636/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=640943319774285636' title='1 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/640943319774285636'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/640943319774285636'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/02/updated-requirements-document.html' title='Updated Requirements Document'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>1</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-5323007633112747421</id><published>2008-02-06T11:27:00.000-05:00</published><updated>2008-02-06T11:28:51.593-05:00</updated><title type='text'>Updated Release &amp; Capabilities Document</title><content type='html'>The FDsys System Releases and Capabilities document has been updated to Version 5.0 and is now available on the FDsys website at &lt;a title=\"http://www.gpo.gov/projects/pdfs/releasesandcapabilities_v5_121907.pdf\" href=\"http://www.gpo.gov/projects/pdfs/releasesandcapabilities_v5_121907.pdf\"&gt;http://www.gpo.gov/projects/pdfs/releasesandcapabilities_v5_121907.pdf&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-5323007633112747421?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/5323007633112747421/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=5323007633112747421' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/5323007633112747421'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/5323007633112747421'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2008/02/updated-release-capabilities-document.html' title='Updated Release &amp; Capabilities Document'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-201198845132732015</id><published>2007-12-11T08:17:00.000-05:00</published><updated>2007-12-11T08:19:21.882-05:00</updated><title type='text'>Join GPO in Florida</title><content type='html'>GPO will be hosting sessions highlighting the FDsys Proof-of-Concept demonstration for both federal agencies and depository libraries December 10 - 13.&lt;div&gt;&lt;br /&gt;&lt;/div&gt;&lt;div&gt;December 11, 2007&lt;span class=\"Apple-tab-span\" style=\"white-space:pre\"&gt; &lt;/span&gt;Ft. Lauderdale, FL&lt;/div&gt;&lt;div&gt;&lt;br /&gt;&lt;/div&gt;&lt;div&gt;December 13, 2007&lt;span class=\"Apple-tab-span\" style=\"white-space:pre\"&gt; &lt;/span&gt;Miami, FL&lt;/div&gt;&lt;div&gt;&lt;br /&gt;&lt;/div&gt;&lt;div&gt;If you are interested in attending, email cgibb@gpo.gov for more information.&lt;/div&gt;&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-201198845132732015?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/201198845132732015/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=201198845132732015' title='3 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/201198845132732015'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/201198845132732015'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2007/12/join-gpo-in-florida.html' title='Join GPO in Florida'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>3</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-1676152681176165868</id><published>2007-12-03T08:57:00.000-05:00</published><updated>2007-12-03T08:58:26.023-05:00</updated><title type='text'>Federal Agencies View Proof-of-Concept</title><content type='html'>GPO gave Federal agencies a first look at the FDsys Proof-of-Concept on November 29. The demonstration was attended by over 30 customers and reactions to the system were extremely positive. Beta testing with federal agencies will occur in early February. If you are interested in beta testing or more information on FDsys, email &lt;a title=\"mailto:pmo@gpo.gov\" href=\"mailto:pmo@gpo.gov\"&gt;pmo@gpo.gov&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-1676152681176165868?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/1676152681176165868/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=1676152681176165868' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1676152681176165868'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/1676152681176165868'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2007/12/federal-agencies-view-proof-of-concept.html' title='Federal Agencies View Proof-of-Concept'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry><entry><id>tag:blogger.com,1999:blog-10013556.post-6483845387321335669</id><published>2007-10-31T07:22:00.000-04:00</published><updated>2007-10-31T07:23:29.388-04:00</updated><title type='text'>Mike Wash on Federal News Radio</title><content type='html'>On October 25, Mike Wash discussed FDsys and the information technology challenges facing GPO on Federal News Radio. To get a better understanding of the need for and scope of FDsys listen to the program online at &lt;a title=\"http://www.federalnewsradio.com/?sid=\" nid=\"34\" href=\"http://www.federalnewsradio.com/?sid=1256242&amp;amp;nid=34\"&gt;http://www.federalnewsradio.com/?sid=1256242&amp;amp;nid=34&lt;/a&gt;.&lt;div class=\"blogger-post-footer\"&gt;&lt;img width='1' height='1' src='https://blogger.googleusercontent.com/tracker/10013556-6483845387321335669?l=fdsys.blogspot.com'/&gt;&lt;/div&gt;</content><link rel='replies' type='application/atom+xml' href='http://fdsys.blogspot.com/feeds/6483845387321335669/comments/default' title='Post Comments'/><link rel='replies' type='text/html' href='https://www.blogger.com/comment.g?blogID=10013556&amp;postID=6483845387321335669' title='0 Comments'/><link rel='edit' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6483845387321335669'/><link rel='self' type='application/atom+xml' href='http://www.blogger.com/feeds/10013556/posts/default/6483845387321335669'/><link rel='alternate' type='text/html' href='http://fdsys.blogspot.com/2007/10/mike-wash-on-federal-news-radio.html' title='Mike Wash on Federal News Radio'/><author><name>Carrie Gibb</name><email>noreply@blogger.com</email><gd:extendedProperty xmlns:gd='http://schemas.google.com/g/2005' name='OpenSocialUserId' value='11787863684005313202'/></author><thr:total xmlns:thr='http://purl.org/syndication/thread/1.0'>0</thr:total></entry></feed>")

